{
  "name": "amazing-store",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "yarn lint:scripts && yarn lint:styles",
    "lint-fix": "yarn lint-fix:scripts",
    "lint:styles": "stylelint src",
    "lint:scripts": "eslint src",
    "lint-fix:scripts": "eslint --fix src",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s docs"
  },
  "dependencies": {
    "next": "^10.0.3",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-modal": "^3.12.1",
    "react-redux": "^7.2.2",
    "redux": "^4.0.5",
    "styled-components": "^5.2.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^9.1.1",
    "@commitlint/config-conventional": "^9.1.1",
    "@storybook/addon-centered": "^5.3.19",
    "@storybook/addons": "^6.3.1",
    "@storybook/react": "^5.3.19",
    "babel-plugin-styled-components": "^1.12.0",
    "eslint": "^7.15.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^4.2.5",
    "stylelint": "^13.8.0",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-declaration-use-variable": "^1.7.2",
    "stylelint-order": "^4.1.0",
    "stylelint-processor-styled-components": "^1.10.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint",
      "pre-push": "yarn lint",
      "commit-msg": "yarn commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
